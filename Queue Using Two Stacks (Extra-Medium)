Queue Using Two Stacks Extra (HackerRank)
https://www.hackerrank.com/challenges/queue-using-two-stacks/problem

//Code passes all test cases on HackerRank

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <stack>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int q;
    cin >> q;
    
    stack<int> stack1, stack2;
    for(int i = 0; i < q; i++){
        int type;
        cin >> type;
        
        if(type == 1){
            int x;
            cin >> x;
            stack1.push(x);
        }
        else if(type == 2){
            if(stack2.empty()){
                while(!stack1.empty()){
                    stack2.push(stack1.top());
                    stack1.pop();
                }
            }
            if(!stack2.empty()){
                stack2.pop();
            }  
        }
        else if(type == 3){
            if(stack2.empty()){
                while(!stack1.empty()){
                    stack2.push(stack1.top());
                    stack1.pop();
                }
            }
            if(!stack2.empty()){
                cout << stack2.top() << endl;
            }
        }
        
    }   
    return 0;
}
